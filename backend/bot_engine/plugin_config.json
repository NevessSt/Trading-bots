{
  "auto_reload": true,
  "validate_plugins": true,
  "allowed_plugin_types": ["strategy", "indicator"],
  "security_checks": true,
  "max_plugins": 100,
  "plugin_timeout": 30,
  "plugin_directories": [
    "plugins/strategies",
    "plugins/indicators",
    "strategies/custom"
  ],
  "default_strategy_parameters": {
    "risk_per_trade": 0.02,
    "max_position_size": 0.1,
    "stop_loss_pct": 0.05,
    "take_profit_pct": 0.1
  },
  "indicator_cache_settings": {
    "enable_cache": true,
    "cache_size_mb": 100,
    "cache_ttl_minutes": 30
  },
  "performance_monitoring": {
    "enable_metrics": true,
    "log_execution_time": true,
    "alert_slow_plugins": true,
    "max_execution_time_ms": 5000
  },
  "plugin_marketplace": {
    "enable_marketplace": true,
    "auto_update_check": false,
    "trusted_sources": [
      "official",
      "community"
    ],
    "require_signature": false
  },
  "development_mode": {
    "enable_hot_reload": true,
    "debug_logging": true,
    "allow_unsigned_plugins": true,
    "plugin_template_directory": "templates"
  },
  "backup_settings": {
    "auto_backup_plugins": true,
    "backup_directory": "backups/plugins",
    "max_backups": 10,
    "backup_before_update": true
  },
  "compatibility": {
    "min_python_version": "3.8",
    "required_packages": [
      "pandas>=1.3.0",
      "numpy>=1.20.0"
    ],
    "optional_packages": [
      "ta-lib",
      "scipy",
      "scikit-learn"
    ]
  },
  "logging": {
    "log_level": "INFO",
    "log_file": "logs/plugin_system.log",
    "max_log_size_mb": 50,
    "log_rotation_count": 5
  }
}