      "secondary": "#FFD700",
      "text_primary": "#FFFFFF",
      "text_secondary": "#CCCCCC",
      "border": "#404040"
    }
  },
  "components": {
    "button": {
      "primary": {
        "background": "linear-gradient(135deg, #00D4FF 0%, #0099CC 100%)",
        "color": "#FFFFFF",
        "border": "none",
        "border_radius": "8px",
        "padding": "12px 24px",
        "font_weight": 600,
        "hover_background": "linear-gradient(135deg, #0099CC 0%, #0066AA 100%)"
      },
      "secondary": {
        "background": "transparent",
        "color": "#0099CC",
        "border": "2px solid #0099CC",
        "border_radius": "8px",
        "padding": "10px 22px",
        "font_weight": 600,
        "hover_background": "#0099CC",
        "hover_color": "#FFFFFF"
      }
    },
    "card": {
      "background": "#FFFFFF",
      "border": "1px solid #E5E5E5",
      "border_radius": "12px",
      "padding": "24px",
      "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)"
    },
    "input": {
      "background": "#FFFFFF",
      "border": "2px solid #E5E5E5",
      "border_radius": "8px",
      "padding": "12px 16px",
      "font_size": "16px",
      "focus_border": "#0099CC",
      "focus_shadow": "0 0 0 3px rgba(0, 153, 204, 0.1)"
    }
  },
  "animations": {
    "duration": {
      "fast": "150ms",
      "normal": "300ms",
      "slow": "500ms"
    },
    "easing": {
      "ease_in": "cubic-bezier(0.4, 0, 1, 1)",
      "ease_out": "cubic-bezier(0, 0, 0.2, 1)",
